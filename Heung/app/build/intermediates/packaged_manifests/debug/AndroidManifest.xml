    <application
        android:name="com.example.heung.GlobalApplication"
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.Heung" >
        <activity
            android:name="com.kakao.sdk.auth.AuthCodeHandlerActivity"
            android:exported="true"
            android:launchMode="singleTask"
            android:theme="@style/TransparentCompat" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="oauth"
                    android:scheme="6de2823164e42a1f96eabbc998a1feb8" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.heung.SelfProfAdapter"
            android:exported="true" />
        <activity
            android:name="com.example.heung.OtherProfActivity"
            android:exported="false" />
        <activity
            android:name="com.example.heung.RentActivity"
            android:exported="false" />
        <activity
            android:name="com.example.heung.LoginActivity"
            android:exported="true"
            android:launchMode="singleTask" > <!-- 이 줄로 이동되었습니다. -->
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.heung.RecrutWriteActivity"
